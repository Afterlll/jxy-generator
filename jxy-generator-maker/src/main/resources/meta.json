{
  "name": "acm-template-pro-generator",
  "description": "ACM 示例模板生成器",
  "basePackage": "com.jxy",
  "version": "1.0",
  "author": "jxy",
  "createTime": "2023-12-4",
  "fileConfig": {
    "inputRootPath": ".source/acm-template-pro",
    "outputRootPath": "generated",
    "sourceRootPath": "D:/code/project/jxy-generator/jxy-generator-demo/acm-template-pro",
    "gitRootPath": "D:/code/project/jxy-generator/.gitignore",
    "type": "dir",
    "files":[
      {
        "inputPath": "src/com/jxy/acm/MainTemplate.java.ftl",
        "outputPath": "src/com/jxy/acm/MainTemplate.java",
        "type": "file",
        "generateType": "dynamic"
      },
      {
        "inputPath": ".gitignore",
        "outputPath": ".gitignore",
        "type": "file",
        "generateType": "static"
      },
      {
        "inputPath": "README.md",
        "outputPath": "README.md",
        "type": "file",
        "generateType": "static"
      }
    ]
  },
  "modelConfig": {
    "models": [
      {
        "fieldName": "loop",
        "type": "boolean",
        "description": "是否循环",
        "defaultValue": "false",
        "abbr": "l"
      },
      {
        "fieldName": "author",
        "type": "String",
        "description": "作者",
        "defaultValue": "jxy",
        "abbr": "a"
      },
      {
        "fieldName": "outputText",
        "type": "String",
        "description": "输出信息",
        "defaultValue": "sum = ",
        "abbr": "o"
      }
    ]
  }
}